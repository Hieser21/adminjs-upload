{
  "name": "example-app",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node --trace-warnings build/src/index.js",
    "clean": "rm -rf build && mkdir build",
    "dev": "yarn clean && concurrently \"wait-on build/src/index.js && nodemon --ext '.js' --watch ../build --watch ./build --ignore 'cypress/**/*.js' node build/src/index.js\" \"yarn build --watch\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "concurrently": "^8.0.1",
    "cypress": "^12.9.0",
    "nodemon": "^2.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@adminjs/design-system": "^4.0.0-beta-v4.8",
    "@adminjs/express": "^6.0.0-beta.3",
    "@adminjs/typeorm": "5.0.0-beta.1",
    "@adminjs/upload": "file:../",
    "@google-cloud/storage": "^5.3.0",
    "adminjs": "7.0.0-beta-v7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "pg": "^8.3.0",
    "typeorm": "^0.3.12"
  },
  "relativeDependencies": {}
}
